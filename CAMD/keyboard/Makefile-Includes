TARGET_PATH = ./include

all: $(LIBRARYNAME)
	-@mkdir -p $(TARGET_PATH)
	cp -R ../include/midi $(TARGET_PATH)
	cp -R ../include/C/* $(TARGET_PATH)

clean:
	-@rm -rf $(TARGET_PATH)

